{
  "name": "template",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check .",
    "format": "prettier --plugin-search-dir . --write .",
    "supabase:types": "supabase gen types typescript --local > ./src/lib/types/supabase.ts"
  },
  "dependencies": {
    "@supabase/auth-helpers-sveltekit": "^0.8.7",
    "@supabase/supabase-js": "^2.7.1",
    "@tallytop/ui": "workspace:*",
    "phosphor-svelte": "^1.2.1",
    "ramda": "^0.28.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.7.0",
    "@tallytop/eslint-config-custom": "workspace:*",
    "@types/ramda": "^0.28.23",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "type": "module"
}
